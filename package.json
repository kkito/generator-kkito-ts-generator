{
  "name": "generator-kkito-ts-generator",
  "version": "0.1.2",
  "description": "yeoman typescript generator ts",
  "homepage": "https://github.com/kkito/generator-kkito-ts-generator",
  "author": {
    "name": "kkito",
    "email": "kkitokkito@126.com",
    "url": "http://kkito.cn"
  },
  "files": [
    "generators/**/*",
    "*.ts"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/__tests__/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint --fix .",
    "test": "jest"
  },
  "repository": "kkito/generator-kkito-ts-generator",
  "license": "Apache-2.0"
}
